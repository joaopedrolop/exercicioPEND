let numero = parseFloat(prompt("Digite um NÃºmero: "));
let soma = 0;

function ehNumeroPerfeito(numero){
    if(numero <= 0){
        return false;
    }
    
     for (let i = 1; i<numero ; i++){
     
         if(numero % i === 0){
             soma = soma + i;
         }
     }
     
     if (soma === numero){
         return true;
     }
     
     return false;
}

console.log(ehNumeroPerfeito(numero));
